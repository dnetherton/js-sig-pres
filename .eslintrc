{
  parser: "babel-eslint",
  rules: {
    strict: 0,
    quotes: [2, "single", "avoid-escape"],
    curly: [2, "multi"],
    "no-mixed-requires": [1, true],
    no-underscore-dangle: 0,
    no-multi-spaces: 0,
    key-spacing: 0,
    no-alert: 0,
    semi: [2, "never"],
    "eqeqeq": ["error", "always"],
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "array-bracket-spacing": 0,
    "max-len": [0, 160, 2],
    "require-jsdoc": 0,
    "no-invalid-this": 0,
    "quote-props": 0,
    "prefer-rest-params": 0,
    "arrow-parens": [0, "as-needed"],
    "padded-blocks": 0,
    "spaced-comment": 0,
    "one-var": 0,
    "brace-style": 0,
    "space-before-function-paren": 0,
    "generator-star-spacing": 0,
    "new-cap": 0,
    "computed-property-spacing": 0,
    "no-console": 0
  },
  globals: {
    console,
    Promise,
    process,
    AppMode,
    fetch,
    setTimeout,
    clearTimeout,
    require,
    ApplicationVersion,
    ApplicationDeploy,
    Symbol,
    IsNode,
    IsChrome,
    document,
    autobind
  },
  plugins: [],
  extends: ["eslint:recommended", "google"]
}
